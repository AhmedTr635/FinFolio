<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="227.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gestioncredit1.SendOffre">
    <children>
        <VBox layoutX="14.0" layoutY="14.0" spacing="10">
            <Label text="Send Offer" />
            <TextField fx:id="userIdTextField" promptText="User ID" />
            <TextField fx:id="creditidTextField" promptText="Credit ID" />
            <TextField fx:id="montantTextField" promptText="Montant" />
            <TextField fx:id="intretTextField" promptText="Intérêt" />
            <Button onAction="#handleSendButton" text="Send" />
        </VBox>
    </children>
</AnchorPane>
